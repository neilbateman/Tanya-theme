.e-scale{
  .dropdown-menu{
    @include scale(0);
    @include transition(all 0.3s ease 0s);
  }
  &.popup-over:hover,&.open{
    .dropdown-menu{
      @include scale(1);
      @include transition(all 0.3s ease 0s);
    }
  } 
}
.e-translate-left{ 
  .popup-content{
    @include opacity(0);
    @include translate(-200px, 0px);
    @include transition(all 0.3s ease 0s);
  }
  &.popup-over:hover{ 
    .popup-content{ 
      @include opacity(1);
      @include translate(0, 0);
      @include transition(all 0.3s ease 0s);
    }
  }
}
.e-translate-right{ 
  .popup-content{
    @include opacity(0);
    @include translate(200px, 0px);
    @include transition(all 0.3s ease 0s);
  }
  &.popup-over:hover{ 
    .popup-content{ 
      @include opacity(1);
      @include translate(0, 0);
      @include transition(all 0.3s ease 0s);
    }
  }
}
.e-translate-top{ 
  .popup-content{
    @include opacity(0);
    @include translate(0, 200px);
    @include transition(all 0.3s ease 0s);
  }
  &.popup-over:hover{ 
    .popup-content{ 
      @include opacity(1);
      @include translate(0, 0);
      @include transition(all 0.3s ease 0s);
    }
  }
}
.e-translate-down{
  .popup-content{
    @include opacity(0);
    height: 0;
    @include transition(all 0.3s ease 0s);
  }
  &.popup-over:hover{ 
    .popup-content{ 
      @include opacity(1);
      height: auto;
      @include transition(all 0.3s ease 0s);
    }
  }
}
.effect {
  text-align: center;
  > a{
    position: relative;
    margin: 0px auto;
    display: block;
    &:before {
      @include transition(all ease 0.3s);
      position: absolute;
      width: 100%;
      content: "";
      left: 0;
      top: 0;
      height: 100%;
    }
    &:hover{
      @include box-shadow(0 0 0 30px rgba(255, 255, 255, 0.2) inset);
      @include transition(all ease 0.3s);
      background: #000;
      &:before {
        @include box-shadow(0 0 0 30px rgba(255, 255, 255, 0.2) inset);
        @include transition(all ease 0.3s);
      }
    }
  }
}
.effect_banner {    
  a{
    position: relative;
    display: block;
    max-width: 100%;
    &:before{
      position: absolute;
      content: "";
      top: 0;
      left: 0;
      margin: auto;
      background-color: rgba(255, 255, 255, 0.1);
      @include size(0,0);
      @include transition (all 0.3s ease-out 0s);
    }
    &:after{
      content: "";
      position: absolute;
      right: 0;
      bottom: 0;
      background-color: rgba(255, 255, 255, 0.1);
      @include size(0,0);
      @include transition (all 0.3s ease-out 0s);
    }
    &:hover:before,
    &:hover:after{
      @include size(100%,100%);
    }
  }
}

