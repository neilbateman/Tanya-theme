.swatch{
  margin:20px 0px 0px; 
  .header {
    @include rtl-margin(0px, 10px, 0px, 0px);
    @include rtl-float-left();
    line-height: 35px;
    min-width: 60px;
    .quick-view-product &{
      width: 100%;
      line-height: normal;
    }
  }
  input { 
    display:none;
  }
  label {
    @include rounded-corners(2px);
    @include rtl-float-left();
    min-width:35px !important; 
    height:35px !important;
    margin:0;
    border: $main-border;
    background-color: $white;
    color: #777;
    /* Styling text */
    font-size:$theme-font-size - 3;
    text-align:center;
    line-height:35px;
    white-space:nowrap;
    text-transform:uppercase;
  }
  .swatch-element {
    @include rtl-float-left();
    -webkit-transform:translateZ(0);
    -webkit-font-smoothing:antialiased;
    margin:0px 10px 0px 0;
    position:relative;
    &.soldout{
      display: none;
    }
  }
}
.swatch-element label { padding:0 10px; cursor: pointer;}
.color.swatch-element label { padding:0; border-radius: 50%;}
.swatch input:checked + label {
  @include box-shadow(0px 1px 4px 1px rgba(0,0,0,0.8));
  border-color:transparent;
} 
.crossed-out { position:absolute; width:100%; height:100%; left:0; top:0; }
.swatch .swatch-element .crossed-out { display:none; }
.swatch .swatch-element.soldout .crossed-out { display:block; }
.swatch .swatch-element.soldout label {
  @include opacity(0.6);
}
.swatch .tooltip {
  text-align:center;
  background:gray;
  color:#fff;
  bottom:100%;
  padding: 10px;
  display:block;
  position:absolute;
  width:100px;
  left:-30px;
  margin-bottom:15px;
  visibility:hidden;
  @include opacity(0);
  @include transform(translateY(10px));
  @include transition(all .25s ease-out);
  @include box-shadow(2px 2px 6px rgba(0, 0, 0, 0.28));
  @include box-sizing(border-box);
}
.swatch .tooltip:before {
  bottom:-20px;
  content:" ";
  display:block;
  height:20px;
  left:0;
  position:absolute;
  width:100%;
}
.swatch .tooltip:after {
  border-left:solid transparent 10px;
  border-right:solid transparent 10px;
  border-top:solid gray 10px;
  bottom:-10px;
  content:" ";
  height:0;
  left:50%;
  margin-left:-13px;
  position:absolute;
  width:0;
}
.swatch .swatch-element:hover .tooltip {
  visibility:visible;
  @include opacity(1);
  @include transform(translateY(0px));
}
.swatch.error {
  background-color:#E8D2D2!important;
  color:#333!important;
  padding:1em;
  border-radius:5px;
}
.swatch.error p {
  margin:0.7em 0;
}
.swatch.error p:first-child {
  margin-top:0;
}
.swatch.error p:last-child {
  margin-bottom:0;
}
.swatch.error code {
  font-family:monospace;
}
